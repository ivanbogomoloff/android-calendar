<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
xmlns:android="http://schemas.android.com/apk/res/android"
android:tag="week_line"
android:layout_gravity="center_vertical"
android:gravity="center_vertical"
android:layout_weight="3"
android:orientation="horizontal"
android:layout_width="match_parent"
android:layout_height="match_parent">
<RelativeLayout
    android:tag="day_num_container"
    android:layout_weight="7"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent">
    <TextView
        android:tag="day_num"
        android:textAlignment="center"
        android:text="2"
        android:layout_weight="7"
        android:gravity="center"
        style="@style/CalendarLayout.WeekNumMonth"/>
    <TextView
        android:visibility="gone"
        android:tag="day_num_selected"
        android:textAlignment="center"
        android:text="2"
        android:gravity="center"
        android:layout_weight="7"
        style="@style/CalendarLayout.WeekNumMonth.Selected"/>
    <TextView
        android:visibility="gone"
        android:tag="day_num_current"
        android:textAlignment="center"
        android:text="2"
        android:gravity="center"
        android:layout_weight="7"
        style="@style/CalendarLayout.WeekNumMonth.Current"/>
    <TextView
        android:tag="day_num_has_event"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text=""
        android:visibility="invisible"
        android:layout_alignParentBottom="true"
        android:gravity="center"
        android:textAlignment="center"
        style="@style/CalendarLayout.WeekNumMonth.HasEvent"
        />
</RelativeLayout>

